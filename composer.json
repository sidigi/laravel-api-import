{
    "name": "sidigi/laravel-api-import",
    "description": "Import data from REST APIs",
    "license": "MIT",
    "authors": [
        {
            "name": "Mikulich Evgeny",
            "email": "sidigicoder@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
